package gui;
import javax.swing.*;

public class MainWindow extends JFrame{

	public MainWindow(){
		setResizable(false);
		setSize(700,500);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setVisible(true);
	}
	
	public static void main(String args[]) {
		new MainWindow();
	}
	
}
