package gui;
import javax.swing.*;

public class MainWindow extends JFrame{

	SelectTypePanel stp;
	
	public MainWindow(){
		int w=700,h=500;
		setResizable(false);
		setSize(w,h);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		stp = new SelectTypePanel(w,h);
		setVisible(true);
	}
	
	public static void main(String args[]) {
		new MainWindow();
	}
	
}
