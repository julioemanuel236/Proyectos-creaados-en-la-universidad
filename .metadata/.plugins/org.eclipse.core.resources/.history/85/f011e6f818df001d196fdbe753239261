package tareas;

import java.util.Scanner;
public class Primos {

	public static void main(String[] args) {
		int cantidad=0;
		for(int i=2;cantidad<5;i++) {
			boolean primo=true;
			for(int j=2;j<=i/2;j++) {
				if(i%j==0)primo=false;
			}
			if(primo) {
				System.out.println(i+" es Primo");
				cantidad++;
			}
		}		
		int ultimoImpar=1;
		for(int i=1;i<=numero;i++) {
			int cubo=0;
			int cantidad=i+1;
			
		}
	}
}
