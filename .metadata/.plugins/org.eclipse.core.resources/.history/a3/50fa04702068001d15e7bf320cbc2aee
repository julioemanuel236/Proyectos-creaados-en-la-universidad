package medios_uci;
import java.util.ArrayList;

public class CONTROL {
	static ArrayList<Medio> medios = new ArrayList<>();
	
	public static void addMedio(Medio m) {
		medios.add(m);
	}
	
	public static void cantidadEquipos() {
		int arr[] = new int[Equipo.MARCA.length];
		for(int i=0;i<medios.size();i++) {
			if(medios.get(i).getClass()==Equipo.class) {
				Equipo e = (Equipo)medios.get(i);
				arr[e.getMarca()]++;
			}
		}
	}
}
