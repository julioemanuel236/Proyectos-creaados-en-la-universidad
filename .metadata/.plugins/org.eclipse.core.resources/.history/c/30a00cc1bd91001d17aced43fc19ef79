package competencia;

public class Pais {
	String nombre;
	String codigo;
	String region;
	public Pais(String nombre, String codigo, String region) {
		super();
		this.nombre = nombre;
		this.codigo = codigo;
		this.region = region;
	}
	public String getNombre() {
		return nombre;
	}
	public String getCodigo() {
		return codigo;
	}
	public String getRegion() {
		return region;
	}
	
	
}
