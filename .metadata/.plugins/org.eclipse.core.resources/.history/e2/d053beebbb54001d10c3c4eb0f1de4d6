package decanato;
import java.util.ArrayList;

public class Licencia extends Alumno{
	ArrayList<Asignatura> repetidas;
	
	public Licencia(String n, String a, String s, String id, String gru, int year, String p, boolean ayuda,Asignatura[] as) {
		super(n, a, s, id, gru, year, p, ayuda);
		for(Asignatura i:as)
			repetidas.add(i);
	}

}
