package gui;
import java.awt.Color;

import javax.swing.*;

public class MainWindow extends JFrame{

	SelectTypePanel stp;
	Reporte repote;
	JPanel table;
	public MainWindow(){
		int w=1120,h=720;
		setResizable(false);
		setSize(w,h);
		setLayout(null);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		stp = new SelectTypePanel(w,h);
		setVisible(true);
		iniNewZone();
	}
	
	private void iniNewZone() {
		
	}
	
	private void iniTableZone() {
		table = new JPanel();
		table.setBackground(Color.red);
		table.setSize(getWidth(),360);
		table.setLocation();
		add(table);
	}
	
	
	public static void main(String args[]) {
		new MainWindow();
	}
	
}
