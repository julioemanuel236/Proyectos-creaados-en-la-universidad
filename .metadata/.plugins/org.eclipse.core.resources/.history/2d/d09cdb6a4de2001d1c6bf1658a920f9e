package Algoritmos;

public class Proceso implements Comparable{
	private String id;
	private int inicio;
	private int tiempo;
	
	public Proceso(String id, int inicio, int tiempo) {
		super();
		this.id = id;
		this.inicio = inicio;
		this.tiempo = tiempo;
	}

	public String getId() {
		return id;
	}

	public int getInicio() {
		return inicio;
	}

	public int getTiempo() {
		return tiempo;
	}
	
	public void procces() {
		tiempo--;
	}
	
	public boolean isTerminated() {
		return tiempo==0;
	}
	

	@Override
	public int compareTo(Object o) {
		Proceso pr = (Proceso) o;
		if(this.tiempo<pr.tiempo)return -1;
		else if(this.tiempo>pr.tiempo)return 1;
		return 0;
	}
}
