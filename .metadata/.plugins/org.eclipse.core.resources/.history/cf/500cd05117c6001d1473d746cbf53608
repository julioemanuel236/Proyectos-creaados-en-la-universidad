package gui;
import java.awt.Image;

import javax.swing.ImageIcon;
import javax.swing.JFrame;
import javax.swing.JPanel;

import app.App;

public class GUI extends JFrame{

	App app;
	TextField nombres = new TextField(150, 30, "Nombres del Estudiante");
	TextField apellidos = new TextField(150, 30, "Apellidos del Estudiante");;
	TextField promedio;
	
	public GUI(App app) {
		this.app=app;
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setIconImage(new ImageIcon(getClass().getResource("/imagenes/Database.png")).getImage());
		setSize(1000,700);
		setLocationRelativeTo(null);
		setResizable(false);
		setVisible(true);
	}
	
}
