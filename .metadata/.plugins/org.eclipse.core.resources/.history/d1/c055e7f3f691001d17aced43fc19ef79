package competencia;

public class Participante {
	
	String nombre;
	String carnet;
	int    edad;
	String sexo;
	Pais pais;
	Equipo equipo;
	
	public Participante(String nombre, String carnet, int edad, String sexo, Pais pais, Equipo equipo) {
		super();
		this.nombre = nombre;
		this.carnet = carnet;
		this.edad = edad;
		this.sexo = sexo;
		this.pais = pais;
		this.equipo = equipo;
	}

	public String getNombre() {
		return nombre;
	}

	public String getCarnet() {
		return carnet;
	}

	public int getEdad() {
		return edad;
	}

	public String getSexo() {
		return sexo;
	}

	public Pais getPais() {
		return pais;
	}

	public Equipo getEquipo() {
		return equipo;
	}
	
	public boolean isJunior() {
		return edad<=8;
	}
	
	
	
}
