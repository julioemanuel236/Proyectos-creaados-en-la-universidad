package project;
public class Garage {

	final private Car parking[];
	
	public Garage(int n) {
		parking = new Car[n];
	}
	
	public Car get(int pos) {
		if(pos<1||pos>parking.length)return null;
		return parking[pos-1];
	}
	
	public void showParking() {
		for(int i=0;i<parking.length;i++) {
			System.out.print("Space "+(i+1)+": ");
			if(parking[i]==null)
		}
		
	}
	
}
