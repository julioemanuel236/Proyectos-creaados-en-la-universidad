package cadena;

public class Reservacion extends Object{
	Cliente cliente;
	float precio;
	String pais;
	public Reservacion(Cliente cliente, float precio,String pais) {		
		this.cliente = cliente;
		this.precio = precio;
		this.pais=pais;
	}
	public Cliente getCLiente() {
		return cliente;
	}
	public float getPrecio() {
		return precio;
	}
	
	public String getPais() {
		return pais;
	}
}
