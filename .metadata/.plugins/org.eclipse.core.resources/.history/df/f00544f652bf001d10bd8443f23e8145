package game;

public abstract class Entity extends java.lang.Object{
	int vida;
	int vidaMaxima;
	int x;
	int y;
	char c;
	public Entity(int vida,char c) {
		this.vida = vidaMaxima = vida;
		this.c=c;
	}

	public void die() {
		Tablero t = App.tablero;
		if()App.t
	}
	
	public String info() {
		return "";
	}

	public boolean isDead() {
		return vida<=0;
	}
	
	public String toString() {
		return ""+c;
	}
}
