package game;

public abstract class Entity extends java.lang.Object{
	int vida;
	int vidaMaxima;
	int x;
	int y;
	char c;
	public Entity(int vida,char c) {
		this.vida = vidaMaxima = vida;
		this.c=c;
	}

	public abstract void die();
	
	public String info() {
		return "";
	}
	
	public String toString() {
		return ""+c;
	}
}
