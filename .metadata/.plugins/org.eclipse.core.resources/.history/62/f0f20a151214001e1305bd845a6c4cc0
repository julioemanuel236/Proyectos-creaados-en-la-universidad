package todolist;

public class Task implements Comparable<Task>{

	private int id;
	
	public Task(int id) {
		this.id = id;
	}
	
	public int getId() {
		return id;
	}
	
	public String toString() {
		return "TASK ID: "+id;
	}
	
	@Override
	public int compareTo(Task o) {
		if(id<o.id)return 1;
		if(id>o.id)return -1;
		return 0;
	}

	
}
