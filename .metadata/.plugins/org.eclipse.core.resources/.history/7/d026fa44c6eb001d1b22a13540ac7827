public class Fixer {

	String fileRute = "50Names.txt";
	ArrayList<Person> persons;
	
	public Fixer() {
		try {
			loadPersons();
		}
		catch(Exception e) {
			
		}
		
	}
	
	public void loadPersons()throws Exception {
		FileReader fr = new FileReader(fileRute);
		persons = new ArrayList<>();
		
	}
	
	
	public static void main(String[] args) {
		new Fixer();
	}
}
