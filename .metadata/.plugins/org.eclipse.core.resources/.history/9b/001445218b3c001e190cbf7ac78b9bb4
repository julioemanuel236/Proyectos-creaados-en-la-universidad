package elementos;

import elementos.serpiente.Cuerpo;

public class Matriz {
	
	private Cuerpo[][] matriz;
	
	public Matriz(int n,int m) {
		matriz = new Cuerpo[n][m];
	}
	
	public int get(int x,int y) {
		if(matriz[y][x] == null)return -1;
		else return matriz[y][x].getId();
	}

