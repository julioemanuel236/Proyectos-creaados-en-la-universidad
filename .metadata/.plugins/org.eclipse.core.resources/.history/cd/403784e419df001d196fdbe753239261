package tareas;

import java.util.Scanner;
public class Primos {

	public static void main(String[] args) {
		int cantidad=0;
		for(int i=2;cantidad<5;i++) {
			boolean primo=true;
			for(int j=2;j<=i/2;j++) {
				if(i%j==0)primo=false;
			}
			if(primo) {
				
				int numeroDelImpar=0;
				int impar=0;
				int numero=impar+1;
				do {
					if(numero%2==1) {
						impar=numero;
						numeroDelImpar++;
					}
				}while(numeroDelImpar<)
				
			}
				cantidad++;
			}
		}		
	
}
